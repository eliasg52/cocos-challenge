{
  "expo": {
    "name": "Cocos Challenge",
    "slug": "cocos-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splashIcon.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/iosIcon.png",
      "bundleIdentifier": "com.eliasg52.cocoschallenge",
      "splash": {
        "image": "./assets/splashIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF",
        "darkMode": {
          "image": "./assets/splashIcon.png",
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/androidIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.eliasg52.cocoschallenge",
      "splash": {
        "image": "./assets/splashIcon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFFFFF",
        "darkMode": {
          "image": "./assets/splashIcon.png",
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-splash-screen"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
